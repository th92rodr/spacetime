generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  githubId  Int    @unique @map("github_id")
  name      String
  login     String
  avatarUrl String @map("avatar_url")

  @@map("users")
}
